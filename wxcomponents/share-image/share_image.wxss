.share-box{
 
}
.draw-item {
  width: 100%;
  height: 100%;
  overflow: hidden;
}
.draw-item image, .draw-item .text {
  position: absolute
}
.share-btn{
  position: absolute;
  width: 100%;
  left: 0;
  bottom: 0;
  z-index: 9999;
}
.share-btn button {
  width: 100%;
  background: #FFFFFF;
  border-radius: 10rpx;
  font-size: 36rpx;
  color: #7D73C3;
  text-align: center
}
